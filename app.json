{
  "expo": {
    "name": "智守 WiseKeep",
    "slug": "wisekeep",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wisekeep",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "xyz.generalize.wisekeep",
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "WiseKeep needs microphone access to record audio for you. 智守需要使用麥克風來為您錄音。",
        "UIBackgroundModes": [
          "audio"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-av",
        {
          "microphonePermission": "WiseKeep needs microphone access to record audio for you. 智守需要使用麥克風來為您錄音。"
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "cedc2597-292b-4966-a03f-00bf4b2ebd42"
      },
      "router": {}
    },
    "owner": "projectrodimus"
  }
}
